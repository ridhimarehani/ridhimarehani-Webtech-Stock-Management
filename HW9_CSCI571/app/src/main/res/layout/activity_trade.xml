<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools">
    <LinearLayout
        android:id="@+id/lProgressBarArea"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="gone" >

        <ProgressBar
            android:id="@+id/ProgressBarMain"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:indeterminateTint="@color/purple_500">
        </ProgressBar>
    </LinearLayout>
    <androidx.core.widget.NestedScrollView
        android:id="@+id/page_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        context=".TradeActivity">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:context=".TradeActivity"
            tools:ignore="MissingClass">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/comp_info">

                <ImageView
                    android:id="@+id/company_logo"
                    android:layout_marginTop="15dp"
                    android:layout_width="60dp"
                    android:layout_height="match_parent"/>
                <RelativeLayout
                    android:layout_width="180dp"
                    android:layout_height="match_parent"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="10dp">

                    <TextView
                        android:id="@+id/ticker_symbol"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginTop="10dp"
                        android:text="TextView"
                        android:textColor="@color/black"
                        android:textSize="25dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/company_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/ticker_symbol"
                        android:layout_marginTop="10dp"
                        android:text="TextView"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="130dp"
                    android:layout_height="match_parent"
                    android:layout_marginTop="10dp"
                    android:gravity="right"
                    android:layout_marginLeft="10dp">

                    <TextView
                        android:id="@+id/latest_price"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginTop="10dp"
                        android:text="TextView"
                        android:textColor="@color/black"
                        android:textSize="25dp"
                        android:gravity="right"
                        android:textAlignment="gravity"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/change_percent"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/latest_price"
                        android:layout_marginTop="10dp"
                        android:gravity="right"
                        android:text="TextView"
                        android:textAlignment="gravity"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold" />
                </RelativeLayout>



            </LinearLayout>

            <RelativeLayout
                android:id="@+id/highcharts"
                android:layout_width="match_parent"
                android:layout_height="430dp"
                android:layout_below="@+id/comp_info">

                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbar"
                    android:layout_width="match_parent"
                    android:layout_height="?attr/actionBarSize" />

                <androidx.viewpager.widget.ViewPager
                    android:id="@+id/viewPager"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/toolbar"
                    android:layout_weight="1" />




            </RelativeLayout>
            <com.google.android.material.tabs.TabLayout
                android:id="@+id/tabLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/highcharts"
                app:tabIconTint="@color/selectedchart"
                app:tabMode="fixed"/>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/portfolio"
                android:layout_below="@+id/tabLayout"
                android:layout_marginTop="30dp">

                <RelativeLayout
                    android:id="@+id/port_heading"
                    android:layout_width="match_parent"
                    android:layout_height="30dp">

                    <TextView
                        android:id="@+id/portfolio_heading"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:textColor="@color/black"
                        android:textSize="20dp"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/shares_details"
                    android:layout_width="260dp"
                    android:layout_height="30dp"
                    android:layout_below="@+id/port_heading"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/shares"

                            android:layout_width="140dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp"
                            android:textColor="@color/black"
                            android:textSize="15dp" />

                        <TextView
                            android:id="@+id/shares_owned"

                            android:layout_width="100dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp"
                            android:textColor="@color/black"
                            android:text="0"
                            android:textSize="15dp" />
                    </LinearLayout>
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/trade_button"
                    android:layout_below="@+id/shares_details">

                    <RelativeLayout
                        android:layout_width="260dp"
                        android:id="@+id/before_trade_button"
                        android:layout_height="match_parent">


                        <RelativeLayout
                            android:id="@+id/avg_cost_details"
                            android:layout_width="260dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginTop="10dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <TextView
                                    android:id="@+id/avg_cost"

                                    android:layout_width="140dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="10dp"
                                    android:textColor="@color/black"
                                    android:textSize="15dp" />

                                <TextView
                                    android:id="@+id/avg_cost_value"
                                    android:text="$0.00"
                                    android:layout_width="100dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="10dp"
                                    android:textColor="@color/black"
                                    android:textSize="15dp" />
                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/total_cost_details"
                            android:layout_width="260dp"
                            android:layout_height="30dp"
                            android:layout_below="@+id/avg_cost_details"
                            android:layout_marginLeft="12dp"
                            android:layout_marginTop="10dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <TextView
                                    android:id="@+id/total_cost"

                                    android:layout_width="140dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="10dp"
                                    android:textColor="@color/black"
                                    android:textSize="15dp" />

                                <TextView
                                    android:id="@+id/total_cost_value"
                                    android:text="$0.00"
                                    android:layout_width="100dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="10dp"
                                    android:textColor="@color/black"
                                    android:textSize="15dp" />
                            </LinearLayout>
                        </RelativeLayout>

                    </RelativeLayout>
                    <LinearLayout
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:layout_toRightOf="@+id/before_trade_button"
                        android:layout_marginLeft="20dp">

                        <androidx.appcompat.widget.AppCompatButton
                            android:layout_marginTop="20dp"
                            android:layout_width="100dp"
                            android:id="@+id/trade_button_value"
                            android:background="@drawable/tradebutton"
                            android:letterSpacing="0.2"
                            android:layout_height="wrap_content"

                            android:textColor="@color/white"
                            android:text="Trade">

                        </androidx.appcompat.widget.AppCompatButton>

                    </LinearLayout>
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/change_details"
                    android:layout_width="260dp"
                    android:layout_height="30dp"
                    android:layout_below="@+id/trade_button"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/change_heading"

                            android:layout_width="140dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp"
                            android:textColor="@color/black"
                            android:textSize="15dp" />

                        <TextView
                            android:id="@+id/change_value"
                            android:text="$0.00"
                            android:layout_width="100dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp"
                            android:textColor="@color/black"
                            android:textSize="15dp" />
                    </LinearLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/market_value_details"
                    android:layout_width="260dp"
                    android:layout_height="30dp"
                    android:layout_below="@+id/change_details"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/market_value_heading"

                            android:layout_width="140dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp"
                            android:textColor="@color/black"
                            android:textSize="15dp" />

                        <TextView
                            android:id="@+id/market_value_Val"
                            android:text="$0.00"
                            android:layout_width="100dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp"
                            android:textColor="@color/black"
                            android:textSize="15dp" />
                    </LinearLayout>
                </RelativeLayout>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/Stats"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/portfolio">

                <RelativeLayout
                    android:id="@+id/stats_head"
                    android:layout_width="match_parent"
                    android:layout_height="30dp">

                    <TextView
                        android:id="@+id/stats_heading"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:textColor="@color/black"
                        android:textSize="20dp"
                        android:textStyle="bold" />
                </RelativeLayout>

                <GridLayout
                    android:id="@+id/stat_details"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/stats_head"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"
                    android:columnCount="2"
                    android:orientation="vertical"
                    android:rowCount="2">

                    <TextView
                        android:id="@+id/open_price"
                        android:layout_rowWeight="1"
                        android:layout_columnWeight="1"
                        android:text="1"
                        android:textColor="@color/black"
                        android:textSize="15dp" />

                    <TextView
