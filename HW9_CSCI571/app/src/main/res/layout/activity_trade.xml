                                    android:layout_width="wrap_content"
                                    android:layout_weight="1"
                                    android:layout_marginBottom="1dp"
                                    android:layout_marginHorizontal="1dp"
                                    android:background="#e0e3e5"
                                    android:textColor="#6b6c6d"
                                    android:textSize="12dp"
